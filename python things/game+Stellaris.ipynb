    "                        name = None\n",
    "                        cost = 0\n",
    "                        tech = None\n",
    "                    except Exception as e:\n",
    "                        print(e)\n",
    "            i += 1\n",
    "\n",
    "make_blocker(glob.glob(target_dir + '\\\\*.txt'))\n",
    "print('done')"
   ]
